RewriteEngine On
RewriteRule ^api/enderecos/([0-9]+)/?$ /api/enderecos.php?id=$1 [L]
RewriteRule ^api/enderecos/([1-9][0-9]{0,3})/?$ /api/enderecos.php?id=$1 [L]
RewriteRule ^enderecos/?$ enderecos.php [L]
RewriteRule ^atendimentos/?$ atendimentos.php [L]
RewriteRule ^setores/?$ setores.php [L]
RewriteRule ^tecnicos/?$ tecnicos.php [L]
RewriteRule ^tipos_atendimento/?$ tipos_atendimento.php [L]
